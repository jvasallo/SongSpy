<h1>You get it all...</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Album</th>
    <th>Length</th>
    <th>Rating</th>
  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= link_to song.name, song %></td>
    <td><%= link_to song.album.name, song.album %></td>
    <td><%= song.length %></td>
    <td><%= song.rating %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Song', new_song_path %>
